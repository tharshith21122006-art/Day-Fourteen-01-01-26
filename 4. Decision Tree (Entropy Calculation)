import math

def entropy(data):
    total = len(data)
    counts = {}
    for label in data:
        counts[label] = counts.get(label, 0) + 1

    ent = 0
    for c in counts.values():
        p = c / total
        ent -= p * math.log2(p)
    return ent

labels = ["Yes","Yes","No","Yes","No","No"]
print("Entropy:", entropy(labels))
